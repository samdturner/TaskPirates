<div class="job-field-container js-job-field-container panel is-focused" data-child="dateAndTime" id="dateAndTime" data-child-status="editing"><div class="" cid="12"><div class="job-form__field job-form__title title-section">
  <label class="job-form__field-label">Your voyage <%= type %> date</label>
</div>
<div class="job-form__field is-editable">
  <div class="job-form__field-inner">
    <form novalidate="">
      <div class="js-container--error"></div>
      <div class="fields-container js-container--fields">
        <div class="date-picker js-error-target" data-key="date slot">
          <a class="date-picker__arrow date-picker__arrow--right js-move js-move-left <%= type %>" data-direction="-1">
            <img class="ss-icon ss-caret-right"
            src="http://i.imgur.com/0cgjsun.png">
          </a>
          <a class="date-picker__arrow date-picker__arrow--left js-move js-move-right <%= type %>" data-direction="1">
            <img class="ss-icon ss-caret-left"
            src="http://i.imgur.com/q5xlrws.png">
          </a>
          <div class="date-picker__date-list-container js-date-list-container">
            <div class="date-picker__date-list js-date-list <%= className %>">

            </div>
          </div>
        </div>
      </div>
      <div>
        <button class="btn btn-primary find-sailors" type="submit">Find a Sailor</button>
      </div>
    </form>
  </div>
</div>
</div>
</div>
